            <hr>
          </div>
          <div class="col-md-12">
            <h2>{{ event.type }} - {{ event.city }}</h2>
            <div class="row">
                <div class="col-md-4"><strong>Informations</strong></div>
                <div class="col-md-8"><p>{{ event.informations }}</p></div>
                <div class="col-md-4"><strong>DÃ©but</strong></div>
                <div class="col-md-8"><p>{{ event.start }}</p></div>
                <div class="col-md-4"><strong>Fin</strong></div>
                <div class="col-md-8"><p>{{ event.end }}</p></div>
            </div>
          </div>
      </div>
  </div>
</body>

</html>